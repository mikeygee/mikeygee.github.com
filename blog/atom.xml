---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Mikey Gee | Blog</title>
 <link href="http://mikeygee.com/blog/atom.xml" rel="self"/>
 <link href="http://mikeygee.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://mikeygee.com/</id>
 <author>
   <name>Mikey Gee</name>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://mikeygee.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://mikeygee.com{{ post.id }}</id>
 </entry>
 {% endfor %}
 
</feed>

